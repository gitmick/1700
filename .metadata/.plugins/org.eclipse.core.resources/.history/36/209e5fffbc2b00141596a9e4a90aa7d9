/**
 * 
 */

function Lemming() {
	this.x=300;
	this.y=0;
	this.circle;
}

Lemming.prototype.create=function() {
	this.circle = new createjs.Shape();
	this.circle.graphics.beginFill("red").drawCircle(this.x, this.y, 40);
	stage.addChild(this.circle);
}

Lemming.prototype.paint=function() {
	this.y++;
}